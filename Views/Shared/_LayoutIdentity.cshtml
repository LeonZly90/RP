<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Resource Planning Tool</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/identity.css" />
    <script src="https://kit.fontawesome.com/f8b0c94c43.js" crossorigin="anonymous"></script>
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3" style="margin-bottom: 5rem;">
            @RenderBody()
        </main>
    </div>

    <footer class="footer text-muted">
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
